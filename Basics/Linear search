#include <stdio.h>

int main()
{
    int a[20],i,j,n,temp,key,flag=0;
    scanf("%d",&n);
    for(i = 1; i <= n; i ++)
     scanf("%d",&a[i]);
    scanf("%d",&key); 
    for(i = 1; i <= n; i ++)
    {
       if(a[i] == key)
       {
        printf("%d position",i);
        flag = 1;
       }
    }
    if(flag == 0)
     printf("No");
    
    return 0;
}
