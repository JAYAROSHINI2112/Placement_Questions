
#include <stdio.h>

int main()
{
    int val1=0,val2=1,temp,n,i;
    printf("Enter the value ");
    scanf("%d",&n);
    printf("\nThe fibonical series valus is \n");
    printf("%d %d ",val1,val2);
    for(i = 2; i < n; i++)
    {
        temp =val1 + val2;
        printf("%d ",temp);
        val1 = val2;
        val2 = temp;
    }
    
    return 0;
}
