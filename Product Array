#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
   int n,k,i,a[20],b[20],pro = 1, sum = 0;
    scanf("%d %d", &n,&k);
    for(i = 0; i < n; i ++)
        scanf("%d",&a[i]);
    for(i = 0; i < n; i ++)
        scanf("%d",&b[i]);
    for(i = 0; i < n; i ++)
    {
        pro = a[i] * b[i];
        sum = sum + pro;
    }
    printf("%d",sum);
    return 0;
}
